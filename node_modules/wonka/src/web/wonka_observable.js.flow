// @flow

import type { Source } from "../wonka_types";
export interface JsSubscription {
  unsubscribe(): void;
}
export interface JsObserver<T> {
  next(value: T): void;
  error(errorValue: any): void;
  complete(): void;
}
export interface JsObservable<T> {
  subscribe(observer: JsObserver<T>): JsSubscription;
}
declare export var fromObservable: <T>(
  observable: JsObservable<T>
) => Source<T>;
declare export var toObservable: <T>(source: Source<T>) => JsObservable<T>;
